# {{ ansible_managed }}
{{ cron_extra_log_file }}
/var/log/cron
/var/log/maillog
/var/log/messages
/var/log/secure
/var/log/spooler
{
    daily
    compress
    delaycompress
    dateext
    ifempty
    missingok
    nocreate
    nomail
    rotate 365
    sharedscripts
    postrotate
	    /bin/kill -HUP `cat /var/run/syslogd.pid 2> /dev/null` 2> /dev/null || true
    endscript
}
